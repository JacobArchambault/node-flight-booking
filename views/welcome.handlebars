<h2>Welcome to Sun or Fun Airlines</h2>
<img src="/images/airplane.jfif" alt="A picture of our aircraft" />
<div role="doc-subtitle">Offering flights from Louisville, KY to Miami and Las Vegas</div>
<table class="table">
    <thead>
        <tr>
            <th>Flight number</th>
            <th>Depart</th>
            <th>Arrive</th>
        </tr>
    </thead>
    <tbody>
        {{#each flights}}
        <tr>
            <td>{{flight}}</td>
            <td>{{origin}}</td>
            <td>{{destination}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
